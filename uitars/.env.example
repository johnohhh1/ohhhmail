# UI-TARS Environment Configuration
# Copy this file to .env and configure for your environment

# WebSocket connection to Dolphin service
REACT_APP_WS_URL=ws://localhost:8080/ws

# API endpoint for REST calls
REACT_APP_API_URL=http://localhost:8080/api

# WebSocket reconnection settings
REACT_APP_WS_RECONNECT_INTERVAL=3000
REACT_APP_WS_MAX_RECONNECT=10

# UI settings
REACT_APP_MAX_EXECUTIONS=100
REACT_APP_LOG_RETENTION_MS=3600000
REACT_APP_THUMBNAIL_SIZE=200

# Polling fallback (if WebSocket fails)
REACT_APP_POLL_INTERVAL=5000

# Feature flags
REACT_APP_DEBUG=false
REACT_APP_PERF_MONITORING=false
